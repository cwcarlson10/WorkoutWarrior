<div id="program-athlete-list-items">
  <% program_athletes.order(:name).uniq.each do |athlete| %>
    <dl class='dl-horizontal'>
      <dt>Name:</dt>
      <dd>
        <%= athlete.name %>
        <%= link_to "Profile", athlete_path(athlete), class: 'btn btn-danger btn-xs pull-right' %>
        <%= link_to "Remove", remove_from_program_path(program, :athlete_id => athlete.id), remote: true, class: 'btn btn-danger btn-xs remove-button pull-right' %>
      </dd>
    </dl>
  <% end %>
</div>